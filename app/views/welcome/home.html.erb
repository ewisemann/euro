body{
margin:0px;
}
/* Multi-Column CSS Menu */
ul.multicolumn {
	width: 33rem;
	left: 17rem;
	margin-bottom: 20px;
	overflow: hidden;
}

ul.multicolumn li {
	margin:0px;
	padding: 0px;
	float: left;
	display: inline;
	width: 33%;
	/* width: 50%; */ /* for 2 columns */
	/* width: 33%; */ /* for 3 columns */
	/* width: 25%; */ /* for 4 columns */
}

ul.multicolumn li a {
	line-height: 1.5rem;
	font-size: 0.8rem;
	padding:2px;
	margin-left: 0.3rem;
}

.text2rem { font-size:1.5rem;}
#nav ul li a.spline2 {font-size: 1.4rem;}
#nav ul li a.spline2l {line-height: 1.4rem;}

#nav .logofake {
display: table-row;
padding: 0.1rem 0.5rem;
}

#nav .logofake li {
display: table-cell;
}

#nav .logotextfake {
font-size: 2rem;
}

#nav .glyphicon{
padding: 0.3rem;
}

#nav td{
vertical-align: middle;
padding: 0.5rem 0.3rem;
}

/***/
#nav .srch-term {display: table-cell;}
#nav .btn {display: table-cell; padding: 0; height: 2rem;font-size:1rem;}

#skipbr {display:none;}

#nav .form-control {
vertical-align: middle;
margin-top: 0px;
margin-bottom: 0px;
height: 2rem;
font-size:1rem;
padding: 0.2rem 0.4rem;
}

#nav .input-group-btn {

vertical-align: middle;
margin-top: auto;
margin-bottom: auto;
font-size: 1rem;
}

#nav .navbar-form{
padding: 2px 2px;
margin-top: 0.8rem;
}

#nav li img {
min-width: 2.5rem;
max-height: 2.5rem;
}

/*Give half space of reguler menu itrem*/
#nav li.semispace {
width:4.5454% !important;
font-weight: 300;
}

/*clean underline of links*/
#nav a, #nav a span {
text-decoration: none;
}
ul, li {
list-style: none;
margin: 0;
padding: 0;
}
#nav {
width: 100%;
position: static;
margin: 0;
font-family: sans-serif;
}
#nav > a {
display: none;
}
#nav li {
position: relative;
}
#nav li a {
color: #fff;
display: block;
}
#nav li a:active {
background-color: #c00 !important;
}
#nav span:not([class]):after {
width: 0;
height: 0;
border: 0.313rem solid transparent; 
border-bottom: none;
border-top-color: #f1f1f1;
content: '';
vertical-align: middle;
display: inline-block;
position: relative;
right: -0.313rem; 
}
/* first level */

#nav > ul {
height: 4.25rem; 
background-color: #5f5f5f;
}


#nav > ul > li {
width: 9.0909%;
height: 100%;
float: left;
}


#nav > ul > li > a {
height: 100%;
font-size: 1.4rem;
line-height: 4.25rem;
text-align: center;
}
/*
#nav > ul > li:hover > a, #nav > ul:not( :hover ) > li.active > a {
background-color: #393939;
font-weight: 700;
}
*/

#nav > ul > li:hover:not(.semispace) > a, #nav > ul:not( :hover ) > li.active > a {
background-color: #393939;
font-weight: 700;
}
/* second level */

#nav li ul {
background-color: #393939;
display: none;
position: absolute;
top: 100%;
}
#nav li:hover ul {
display: block;
left: 0;
right: 0;
}
#nav li:not( :first-child ):hover ul {
left: -1px;
}
#nav li ul a {
font-size: 0.85rem; 
border-top: 1px solid #5f5f5f;
padding: 0.6rem; 
}
#nav li ul li a:hover, #nav li ul:not( :hover ) li.active a {
background-color: #5f5f5f;
}
 
 @media only screen and ( max-width: 80rem ) /* 1280 */ {
#nav ul li a.spline2 {line-height: 1.5rem; font-size: 1.4rem; padding-top: 0.6rem;}
#skipbr {display:initial;}
}

 @media only screen and ( max-width: 62.5rem ) /* 1000 */ {
#nav {
font-size: 0.8rem; 
width: 100%;
position: static;
margin: 0;
}
ul.multicolumn li {
	width: 50%;
}
ul.multicolumn {
